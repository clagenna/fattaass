<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="599.0" prefWidth="920.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sm.clagenna.fattaass.javafx.FattAassController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem fx:id="mnuRescanDirs" mnemonicParsing="false" onAction="#mnuRescanDirsClick" text="Rescan Dir" />
                  <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="mnuExit" mnemonicParsing="false" onAction="#mnuExitClick" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem fx:id="mnuEditIntesta" mnemonicParsing="false" onAction="#mnuEditIntestaClick" text="Edit Intesta" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem fx:id="mnuHelpFattAass" mnemonicParsing="false" text="Circa Fatt. AASS" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="466.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="13.0" layoutY="13.0" text="Titolare Fatt." />
            <ComboBox fx:id="cbIntesta" layoutX="88.0" layoutY="10.0" onAction="#cbIntestaClick" prefHeight="25.0" prefWidth="213.0" AnchorPane.leftAnchor="88.0" AnchorPane.topAnchor="10.0" />
            <Label layoutX="14.0" layoutY="46.0" text="Dir Fatture" AnchorPane.leftAnchor="14.0" />
            <TextField fx:id="txDirFatt" layoutX="88.0" layoutY="42.0" onAction="#onEnterDirPDF" onKeyReleased="#premutoTasto" prefHeight="25.0" prefWidth="756.0" AnchorPane.leftAnchor="88.0" AnchorPane.rightAnchor="76.0" />
            <Button fx:id="btCercaDir" layoutX="852.0" layoutY="42.0" mnemonicParsing="false" onAction="#btCercaClick" text="Cerca..." AnchorPane.rightAnchor="14.0" />
            <CheckBox fx:id="ckGenHtml" layoutX="14.0" layoutY="79.0" mnemonicParsing="false" text="Gen HTML File">
               <tooltip>
                  <Tooltip text="Viene generato il file HTML dopo l'interpretazione del documento PDF. Inoltre vengono evidenziati i campi trascurati dalle sequenze" />
               </tooltip>
            </CheckBox>
            <CheckBox fx:id="ckGenTXT" layoutX="120.0" layoutY="79.0" mnemonicParsing="false" text="Genera TXT file">
               <tooltip>
                  <Tooltip text="Genera un file con il solo testo che è stato interpretato del file  PDF" />
               </tooltip></CheckBox>
            <CheckBox fx:id="ckGenCSV" layoutX="247.0" layoutY="79.0" mnemonicParsing="false" text="Genera CSV file">
               <tooltip>
                  <Tooltip text="Viene generato il file con tutti i TAG dopo interpretazione del PDF, Inoltre se in TRACE mode viene scritto il trace delle associazioni delle sequenze" />
               </tooltip></CheckBox>
            <CheckBox fx:id="ckOverwrite" layoutX="423.0" layoutY="79.0" mnemonicParsing="false" text="Sovrascrivi Fatture">
               <tooltip>
                  <Tooltip text="Se la fattura esiste già nel DB essa viene **SOVRASCRITTA** da questa nuova lettura" />
               </tooltip></CheckBox>
            <CheckBox fx:id="ckLanciaExcel" layoutX="587.0" layoutY="79.0" mnemonicParsing="false" text="Lancia Excel su CSV">
               <tooltip>
                  <Tooltip text="Se la fattura esiste già nel DB essa viene **SOVRASCRITTA** da questa nuova lettura" />
               </tooltip>
            </CheckBox>
            <Button fx:id="btConvPDF" layoutX="63.0" layoutY="109.0" mnemonicParsing="false" onAction="#btImportClick" prefHeight="25.0" prefWidth="131.0" text="Import PDF">
               <tooltip>
                  <Tooltip text="Esegue l'import di tutti i files che sono selezionati nella sottostante lista" />
               </tooltip>
            </Button>
            <Button fx:id="btShowResults" layoutX="215.0" layoutY="109.0" mnemonicParsing="false" onAction="#btDatiClick" prefHeight="25.0" prefWidth="131.0" text="Mostra Dati">
               <tooltip>
                  <Tooltip text="Mostra la form (anche piu' di una) d'interrogazione dati" />
               </tooltip>
            </Button>
            <SplitPane fx:id="spltPane" dividerPositions="0.7" layoutX="9.0" layoutY="143.0" orientation="VERTICAL" prefHeight="431.0" prefWidth="905.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="9.0" AnchorPane.rightAnchor="6.0" AnchorPane.topAnchor="143.0">
              <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <TableView fx:id="tblFatt" layoutY="120.0" prefHeight="319.0" prefWidth="903.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                          <columns>
                            <TableColumn fx:id="colTpFatt" prefWidth="60.0" text="Tipo" />
                              <TableColumn fx:id="colAnnoComp" prefWidth="60.0" text="Anno" />
                            <TableColumn fx:id="colDtEmiss" prefWidth="120.0" text="Emissione" />
                              <TableColumn fx:id="colDtIniz" prefWidth="120.0" text="Inizio" />
                              <TableColumn fx:id="colDtFine" prefWidth="120.0" text="Fine" />
                              <TableColumn fx:id="colTotFattura" prefWidth="100.0" text="Importo" />
                              <TableColumn fx:id="colFullPath" prefWidth="280.0" text="Nome File" />
                              <TableColumn fx:id="colStatus" maxWidth="200.0" prefWidth="60.0" text="Status" />
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <TableView fx:id="tblLogs" layoutY="-1.0" prefHeight="82.0" prefWidth="561.0" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="88.0" AnchorPane.topAnchor="0.0">
                          <columns>
                            <TableColumn fx:id="colTime" prefWidth="75.0" text="Momento" />
                            <TableColumn fx:id="colLev" prefWidth="75.0" text="Liv.Err" />
                              <TableColumn fx:id="colMsg" prefWidth="450.0" text="Messaggio" />
                          </columns>
                        </TableView>
                        <Button fx:id="btClearMsg" layoutX="593.0" layoutY="14.0" mnemonicParsing="false" onAction="#btClearMsgClick" text="Clear" AnchorPane.rightAnchor="13.0" />
                        <ComboBox fx:id="cbLevelMin" layoutX="568.0" layoutY="54.0" onAction="#cbLevelMinSel" prefHeight="25.0" prefWidth="84.0" AnchorPane.rightAnchor="2.0" />
                        <Label fx:id="lbProgressione" layoutX="1.0" layoutY="90.0" prefHeight="17.0" prefWidth="99.0" text="Processo..." AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="804.0" />
                        <ProgressBar fx:id="progressBar" layoutX="101.0" layoutY="105.0" prefHeight="18.0" prefWidth="802.0" progress="0.0" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="101.0" AnchorPane.rightAnchor="0.0" />
                     </children>
                  </AnchorPane>
              </items>
            </SplitPane>
      </children>
    </AnchorPane>
  </children>
</VBox>
